  },
  "homepage": "https://gitlab.com/rossyfranca/mysql-tsnode-api#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai-http": "^3.0.5",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/mysql": "^2.15.5",
    "@types/nodemailer": "^4.6.5",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
    "mysql": "^2.16.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.18.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
